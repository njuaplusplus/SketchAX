int[2*8] dropThirdSK1(int[3*8] in) {
    int[2*8] out;
    int i = 0;
    int j = 0;

    loop(8) {
        out[getIndex(j)] = in[getIndex(i)];
        out[getIndex(j)] = in[getIndex(i)];
        i += ??;
        j += 2;
    }
    return out;
}

generator int getIndex(int i) {
    if(??) return i+??;
    if(??) return i++;
    if(??) return ++i;
