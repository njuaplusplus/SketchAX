//@Description Counts the number of ones in a bit-vector in logn steps.
int W=4;

int countGreater(int[W] in, int v)
{
    int ret = 0;
    for(int i=0;i<W;i++){
        if(in[i] > v)
            ret = ret + 1;
    }
    return ret;
}

