int W=8;

bit[W] countBits(bit[W] in)
{
	bit[W] ret=0;
	bit[W] tmp = 0;
	tmp[0] = 1;
	for(int i=0;i<W;i++){
		if(in[i])
			ret=ret+tmp;
	}
	return ret;
}



bit[W] countSketch2(bit[W] in) implements countBits
{
	bit[W] tmp=in;
	loop({*})
	{
		bit[W] m={*};
		tmp=(tmp & m) + (tmp>>{*} & m);
	}
	return tmp;
}

