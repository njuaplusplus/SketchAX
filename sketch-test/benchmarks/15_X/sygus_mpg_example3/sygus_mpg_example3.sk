int select(int x, int y, int z) {
    if (3*z >= 5) {
        if (2*x <= y) {
            return 3*x-5*y+7*z+9;
        } else {
            return 2*x-9*z+5;
        }
    } else {
        if (2*z <= -y + 2*x) {
            return -6*x + 3*y+4;
        } else {
            return 9*x+9*y-z+5;
        }
    }
}

