int N = 3;

generator int index(int i, int j){ /* automatically rewritten */
    return i*N + j;
}

int[N*N] transpose(int[N*N] input1){
    int[N*N] output=0;
    for(int i=0; i<N; ++i){
        for(int j=0; j<N; ++j){
            output[index(i,j)] = input1[index(j,i)];
        }
    }
    return output;
}

